<div class="col-12 justify-content-center">

  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <h2>{{user.attendee_name}}</h2>
      </div>
    </div>
  </div>
  <div class="container">
    <h5>Attending Events: </h5>
    <div id="user">
      <ul>
        {{#each events}}
        <li>
          {{this.event_name}}{{this.event_location}}
        </li>
        {{/each}}
      </ul>
    </div>
    <h5>Interested in: </h5>
    <ul>
      {{#each interests}}
      <li>
        {{this.interest}}{{this.interest_desc}}
      </li>
      {{/each}}
    </ul>
  </div>
</div>
</div>
</div>