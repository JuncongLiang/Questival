<h1>Attendees</h1>

<div id="usersList" class="d-flex flex-wrap justify-content-center">

  {{#each users}}
  <div id="listCards" class="card m-3">
    <div class="card-body">
      <h2 class="card-title"></h2><a href="/users/{{this.id}}"> {{this.attendee_name}}</a>

    </div>
  </div>
  {{/each}}
</div>

<div class="d-flex justify-content-center pt-5">
  <a href="/user/new" class="btn btn-dark btn-lg dark p-4" role="button" aria-pressed="true">Create a
    new attendee!</a>
</div>