<div id="jumbotronMain" class="jumbotron jumbotron-fluid">
    <div class="container d-flex justify-content-center">
        <h1 class="display-4">{{event.event_name}}</h1>
    </div>
</div>

{{event.event_name}}
<p>Description: <br> {{event.event_desc}}</p>
<p>Dates: <br> {{event.event_date}}</p>
<p>Location: <br> {{event.event_location}}</p>


<div class="d-flex flex-wrap justify-content-center">
    <div class="card m-5" style="width: 18rem;">
        <div class="card-header">
            <h2>Attendee's</h2>
        </div>
        <ul class="list-group list-group-flush">
            <li class="list-group-item">
                <ul>
                    {{#each users}}
                    <li>
                        {{attendee_name}}
                    </li>
                    {{/each}}
                </ul>
            </li>
        </ul>
    </div>

    <div class="card m-5" style="width: 18rem;">
        <div class="card-header">
            <h2>Interest's</h2>
        </div>
        <ul class="list-group list-group-flush">

            <li class="list-group-item">
                <ul>
                    {{#each interests}}
                    <li>
                        {{interest}}
                    </li>
                    {{/each}}
                </ul>
            </li>
        </ul>
    </div>
</div>